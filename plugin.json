{
  "pluginName": "编程小助手",
  "description": "解决自己在写代码时遇到的一些小问题。",
  "logo": "img/paopao.jpg",
  "preload": "preload.js",
  "version": "0.5.0",
  "author": "小金鱼",
  "homepage": "https://github.com/ljynfo/utools-code-helper",
  "platform": [
    "win32",
    "darwin",
    "linux"
  ],
  "features": [
    {
      "code": "codeHelper",
      "explain": "编程小助手",
      "cmds": [
        "code helper",
        "Java IDEA",
        "代码 编程 助手",
        "格式化log日志参数",
        "toString转Json并格式化",
        "格式化Json串",
        "去字符拼接",
        "复制文本",
        "计算一列数字",
        "拆解任务提取",
        {
          "type": "regex",
          "label": "编程小助手",
          "match": "/.*?/",
          "minNum": 1
        }
      ]
    }
  ],
  "pluginSetting": {
    "single": true
  }
}